Algoritmo Calcular_Area_Rectangulo
	
	Definir base Como Real;
	Definir altura Como Real;
	
	Escribir "Introduzca el valor de la base del rectangulo";
	Leer base;
	
	Escribir "Introduzca el valor de la altura del rectangulo";
	Leer altura;
	
	Escribir "El area del rectangulo de base ", base, " y altura ", altura, " es igual a: ", base * altura;
	
FinAlgoritmo
