Algoritmo Suma_Resta_Multiplicar_Dividir
	
	Definir A Como Real;
	Definir B Como Real;
	
	Escribir "Introduzca el 1er con el que desea operar";
	Leer A;
	
	Escribir "Introduzca el 2do con el que desea operar";
	Leer B;
	
	Escribir "Suma de ", A, " y ", B, " es igual a: ", A + B;
	Escribir "Resta de ", A, " y ", B, " es igual a: ", A - B;
	Escribir "Multiplicacion de ", A, " y ", B, " es igual a: ", A * B;
	Escribir "Division de ", A, " y ", B, " es igual a: ", A / B;
	
FinAlgoritmo
