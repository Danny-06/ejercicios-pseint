Algoritmo Multiplicar_2_numeros_dados
	
	Definir A Como Real;
	Definir B Como Real;
	
	Escribir "Introduzca el 1er número que desea multiplicar.";
	Leer A;
	
	Escribir "Introduzca el 2do número que desea multiplicar.";
	Leer B;
	
	Escribir "El resultado de la multiplicación es: ", A*B;
	
FinAlgoritmo
